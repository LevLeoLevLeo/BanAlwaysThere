<Page x:Class="BanAlwaysThere.Pages.PageContact"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BanAlwaysThere.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="1565"
      Title="PageContact">

    <Grid Background="#F5F5F5">

        <Grid.ColumnDefinitions>

            <ColumnDefinition Width="888"/>
            <ColumnDefinition Width="*"/>

        </Grid.ColumnDefinitions>

        <TextBlock Style="{StaticResource TxtTitle}" HorizontalAlignment="Left" VerticalAlignment="Top" Text="Контакты" Margin="22 37 0 0"/>



        <TextBox Style="{StaticResource TxtProfileBox}" Text="Search for message or users..." Margin="22 -100 19 0" Height="61" Width="847" Background="#07CC85"/>

    </Grid>

</Page>
